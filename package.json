    "LICENSE",
    "build/index.js",
    "CHANGELOG.md"
  ],
  "main": "build/index.js",
  "engines": {
    "node": "13.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jGleitz/markdown-it-kbd.git"
